void	CtmControl_Quality::SetMoniLastToRefer()
{
	long Last 	= 0;
	for (int i = 0; i < sizeof(m_pszReferID) / sizeof(char*); i++)
	{
		Last = GetDBValue(m_pszLastID[i]).lValue;

		SetDBValue(m_pszReferID[i], Last);
	}

	if(CtmConfig::GetInstance()->GetMachineType() & MACHINE_TYPE_OIL_DC) 
	{
		for (int i = 0; i < sizeof(m_pszReferIDB) / sizeof(char*); i++)
		{
			Last = GetDBValue(m_pszLastIDB[i]).lValue;
	
			SetDBValue(m_pszReferIDB[i], Last);
		}
	}
