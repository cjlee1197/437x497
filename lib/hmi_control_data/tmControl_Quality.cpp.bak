void	CtmControl_Quality::SetLimit()
{
	long Value = 0;
	long Pct 	= 0;	//	percent;
	//long Last 	= 0;	//	last
	long Delta 	= 0;	//	delta
	long Refer  = 0;
	for (int i = 0; i < sizeof(m_pszLimitUpID) / sizeof(char*); i++)
	{
		Pct = GetDBValue(m_pszPctID[i]).lValue;
		Delta = GetDBValue(m_pszDeltaID[i]).lValue;
		//Last = GetDBValue(m_pszLastID[i]).lValue;
		Refer = GetDBValue(m_pszReferID[i]).lValue;

		Value = (long)(Refer * (1 + (double)Pct / 100.0) + Delta);
		SetDBValue(m_pszLimitUpID[i], Value);
		
		Value = (long)(Refer * (1 - (double)Pct / 100.0) - Delta);
		if (Value < 0) Value = 0;
		SetDBValue(m_pszLimitLowerID[i], Value);
	}
}

