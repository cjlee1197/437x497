void	CtmControl_Quality::GetDataOil(int nType, tmOIL_MONI2EX*	pMoni2, double* pData)
{
	switch(nType)
	{
		case MONI_OIL_SHOTCOUNT_ID:								//0	開模序號
			*pData = pMoni2->dwShotCount;
			//cout<<"=======[0]"<<*pData<<endl;     			
			break;                            			
		case MONI_OIL_CYCLE_ID:										//1	循環計時值
			*pData = pMoni2->tmlCycle;
			//cout<<"=======[1]"<<*pData<<endl;          			
			break;                            			
		case MONI_OIL_INJECT_ID:									//2	射出計時值 
			*pData = pMoni2->tmlInject;
			//cout<<"=======[2]"<<*pData<<endl;         			
			break;                            			
		case MONI_OIL_TURNTOHOLD_ID:							//3	保壓轉換時間實際值 
			*pData = pMoni2->tmlTurnToHold;
			break;                            			
		case MONI_OIL_CHARGE_ID:									//4	儲料計時值 
			*pData = pMoni2->tmlCharge;  
			//cout<<"=======[4]"<<*pData<<endl;      		                   
			break;                            		
		case MONI_OIL_PRES_TURNTOHOLD_ID:					//5	保壓轉換壓力值 
			*pData = pMoni2->daPres_TurnToHold; 		                         
			break;                              		
		case MONI_OIL_POSI_INJECTSTART_ID:				//6	射出起點
			*pData = pMoni2->adPosi_InjectStart;
			//cout<<"=======[6]"<<*pData<<endl;		  
			break;                              		
		case MONI_OIL_POSI_TURNTOHOLD_ID:					//7	保壓起點 
			*pData = pMoni2->adPosi_TurnToHold;
			//cout<<"=======[7]"<<*pData<<endl;   	
			break;                                	
		case MONI_OIL_POSI_INJECTEND_ID:					//8	射出終點
			*pData = pMoni2->adPosi_InjectEnd;    	
			break;                                	
		case MONI_OIL_POSI_INJECTCUSHION_ID:			//9	射出監控位置
			*pData = pMoni2->adPosi_InjectCushion;
			//cout<<"=======[9]"<<*pData<<endl;	
			break;                                	
		case MONI_OIL_CHARGERPM_ID:								//10儲料RPM 值
			*pData = pMoni2->cnChargeRPM;     			
			break;                            			
		case MONI_OIL_SUCKBACK_ID:								//11射退計時值
			*pData = pMoni2->tmSuckBack;      			
			break;                            			
		case MONI_OIL_EJECT_ID:										//12脫模計時值
			*pData = pMoni2->tmEject;         			
			break;                            			
		case MONI_OIL_EJECTADVANCE_ID:						//13脫進計時值  
			*pData = pMoni2->tmEjectAdvance;  		
			break;                            		
		case MONI_OIL_EJECTRETURN_ID:							//14脫退計時值  
			*pData = pMoni2->tmEjectReturn;       	
			break;                                	
		case MONI_OIL_PRES_INJECTPEAK_ID:					//15射出尖峰壓力 
			*pData = pMoni2->daPres_InjectPeak;   	
			break;                                	
		case MONI_OIL_PRES_CHARGEPEAK_ID:					//16儲料尖峰壓力  
			*pData = pMoni2->daPres_ChargePeak;   	
			break;                                	
		case MONI_OIL_INJECTB_ID:									//17B組射出計時值
			*pData = pMoni2->tmlInjectB;      			
			break;                            			
		case MONI_OIL_CHARGEB_ID:									//18B組儲料計時值
			*pData = pMoni2->tmlChargeB;      			
			break;                            			
		case MONI_OIL_POSI_INJECTSTARTB_ID:				//19B組射出起點
			*pData = pMoni2->adPosi_InjectStartB; 	
