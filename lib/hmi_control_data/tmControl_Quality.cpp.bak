void*	CtmControl_Quality::GetMachineDataType(int nDataType, int nIndex)
{
	tmOIL_MONI2EX*	pMoni2 = NULL;
	switch(nDataType)
	{
		case MONI_DATATYPE_REAL_ID:					//0	real value
			GetReal();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_LIMITUP_ID:			//1	LimitUP
			GetLimitUp();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_LIMITLOWER_ID:		//2	limit lower
			GetLimitLower();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_FILEREAL_ID:			//3	文件中的實際值
			GetFileReal(nIndex);
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_STANDARDAVG_ID:	//4	標準差
			GetStandardAvg();
			pMoni2 = &m_tmMoni2;
			break;			
		case MONI_DATATYPE_SIG_ID:					//5	sig
			GetSig();
			pMoni2 = &m_tmMoni2;
			break;			
		case MONI_DATATYPE_AVG_ID:					//6	平均值
			GetAvg();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_AVGLIMITUP_ID:		//7	平均值的上限(平均值加上標準差)
			//GetStandardAvg();
			GetAvgLimitUp();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_AVGLIMITLOWER_ID://8	平均值的下限(平均值減去標準差)
			GetAvgLimitLower();
			pMoni2 = &m_tmMoni2;
			break;			
		case MONI_DATATYPE_SUM_ID:					//9	sum
			GetSum();
			pMoni2 = &m_tmMoni2;
			break;
		case MONI_DATATYPE_SQRSUM_ID:				//10	平方和
			GetSqrSum();
			pMoni2 = &m_tmMoni2;
			break;
	}
	
	return pMoni2;
}

